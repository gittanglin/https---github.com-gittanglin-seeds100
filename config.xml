<?xml version='1.0' encoding='utf-8'?>
<widget id="cn.seeds" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>seeds100</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon src="www/img/logo.png" density="ldpi"/>
        <icon src="www/img/logo.png" density="mdpi"/>
        <icon src="www/img/logo.png" density="hdpi"/>
        <icon src="www/img/logo.png" density="xhdpi"/>

        <splash src="www/img/splash.png" density="land-hdpi"/>
        <splash src="www/img/splash.png" density="land-ldpi"/>
        <splash src="www/img/splash.png" density="land-mdpi"/>
        <splash src="www/img/splash.png" density="land-xhdpi"/>
        <splash src="www/img/splash.png" density="port-hdpi"/>
        <splash src="www/img/splash.png" density="port-ldpi"/>
        <splash src="www/img/splash.png" density="port-mdpi"/>
        <splash src="www/img/splash.png" density="port-xhdpi"/>
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <icon src="www/img/logo.png" width="180" height="180"/>
        <icon src="www/img/logo.png" width="60" height="60"/>
        <icon src="www/img/logo.png" width="120" height="120"/>
        <icon src="www/img/logo.png" width="76" height="76"/>
        <icon src="www/img/logo.png" width="152" height="152"/>
        <icon src="www/img/logo.png" width="40" height="40"/>
        <icon src="www/img/logo.png" width="80" height="80"/>
        <icon src="www/img/logo.png" width="57" height="57"/>
        <icon src="www/img/logo.png" width="114" height="114"/>
        <icon src="www/img/logo.png" width="72" height="72"/>
        <icon src="www/img/logo.png" width="144" height="144"/>
        <icon src="www/img/logo.png" width="29" height="29"/>
        <icon src="www/img/logo.png" width="58" height="58"/>
        <icon src="www/img/logo.png" width="50" height="50"/>
        <icon src="www/img/logo.png" width="100" height="100"/>

        <splash src="www/img/splash.png" width="320" height="480"/>
        <splash src="www/img/splash.png" width="640" height="960"/>
        <splash src="www/img/splash.png" width="768" height="1024"/>
        <splash src="www/img/splash.png" width="1536" height="2048"/>
        <splash src="www/img/splash.png" width="1024" height="768"/>
        <splash src="www/img/splash.png" width="2048" height="1536"/>
        <splash src="www/img/splash.png" width="640" height="1136"/>
        <splash src="www/img/splash.png" width="750" height="1334"/>
        <splash src="www/img/splash.png" width="1242" height="2208"/>
        <splash src="www/img/splash.png" width="2208" height="1242"/>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <preference name="SplashScreenDelay" value="1000"/>
</widget>
